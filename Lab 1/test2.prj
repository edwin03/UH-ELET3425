# BiPOM Electronics Project File - DO NOT EDIT
#
# Project file version
#
VERSION=1.04
AUTHOR=BiPOM
#
# Toolkit identification and environment
#
TOOLKIT DIR={ROOTDIR}\microc
TOOLKIT NAME=Micro C 8051-8052
TOOLKIT ENV=MCDIR={TOOLKITDIR}
TOOLKIT 16BIT=YES
#
# Micro-Controller Loader identification
#
LOADER NAME=MINI-MAX/51-C Loader
#
# What to do when we do builds
#
BUILD ENABLED=YES
DOWNLOAD ENABLED=NO
RUN MODE ENABLED=NO
#
# Toolkit specific commands
#
PREBUILD COMMAND=
POSTBUILD COMMAND={TOOLKITDIR}\asm51 {TMPFILE} -I -T -Q
POSTBUILD ALWAYS EXECUTE=ON ERROR
COMPILE COMMAND={TOOLKITDIR}\cc51.com {SOURCEFILE} -A -i -p -C -Q -L -S m=medium
ASM COMMAND=
LINK COMMAND={TOOLKITDIR}\slink {OBJFILES} l={TOOLKITDIR}\lib51 i=bipomm.lib -q {TMPFILE}
LINK EXTENSION=asm
OUTPUT COMMAND={TOOLKITDIR}\asm51 {TMPFILE} -I -F -S
ASM EXTENSION=
OBJ EXTENSION=asm
OUTPUT FILE=
OUTPUT EXTENSION=hex
DEBUGGER=8051 Simulator
CHIP_ID=8052
MANUFACTURER_ID=Generic
OPEN_LISTING=NO
SIMULATE_INTERRUPTS=YES
SIMULATE_TIMERS=YES
SIMULATE_COMPORTS=YES
SIMULATE_PORTS_HW=YES
#
# Project sources
#
SOURCE=.\test2.c
#
# Project plugins
#